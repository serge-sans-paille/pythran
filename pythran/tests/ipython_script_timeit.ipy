from IPython import get_ipython
ipython = get_ipython()
ipython.magic('%load_ext pythran.magic')
ipython.run_cell_magic('pythran', '-O2', '''
#pythran export foo(int)
def foo(n):
    for i in range(n):
        i = 1
    return 0
''')
ipython.run_line_magic('timeit', 'foo(1000)')
ipython.run_cell_magic('pythran', '-Ofast', '''
#pythran export foo(int)
def foo(n):
    for i in range(n):
        i = 1
    return 0
''')
ipython.run_line_magic('timeit', 'foo(1000)')
